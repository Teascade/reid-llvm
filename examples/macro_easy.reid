import std::String;
import std::print;

fn main() -> u8 {
    let bytes = include_bytes!("./macro_easy_file.txt");
    print(String::new() + bytes.length());
    print(String::new() + (include_bytes!("./macro_easy_file.txt") as *u8)[1] as u64);
    return (include_bytes!("./macro_easy_file.txt") as *u8)[0];
}
