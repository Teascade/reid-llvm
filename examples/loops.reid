// Arithmetic, function calls and imports!

import std::print;
import std::from_str;
import std::add_num_to_str;
import std::free_string;

fn main() -> u32 {
  for i in 0 .. 15 {
    let mut text = from_str("num: ");
    add_num_to_str(&mut text, i);
    print(text);
    free_string(&text);
  }

  let mut num = 0;
  while num < 10 {
    num = num + 1;
  }

  return num;
}
