import std::print;
import std::new_string;
import std::add_char;
import std::free_string;

fn main() -> i32 {
    let mut test = new_string();
    add_char(&mut test, 'h');
    add_char(&mut test, 'e');
    add_char(&mut test, 'l');
    add_char(&mut test, 'l');
    add_char(&mut test, 'o');
    
    print(&test);

    free_string(&mut test);

    return 0;
}
